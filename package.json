{
  "name": "react-swagchat",
  "version": "0.9.4",
  "description": "swagchat UIKit - A set of React components for swagchat -",
  "keywords": [
    "swagchat",
    "chat",
    "messaging",
    "sdk",
    "typescript",
    "uikit",
    "react"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:lib": "./node_modules/.bin/tsc --pretty --watch",
    "build:css": "./node_modules/.bin/tcm src -c --pretty --watch",
    "build:dev": "./node_modules/.bin/webpack --env=dev --progress --profile --colors --watch",
    "build:prod": "./node_modules/.bin/webpack --env=prod --progress --profile --colors --watch",
    "cpx:css": "./node_modules/.bin/cpx 'src/**/*.css*' lib",
    "test": "./node_modules/.bin/jest --coverage",
    "publish": "rm -rf lib && ./node_modules/.bin/cpx 'src/**/*.css*' lib && ./node_modules/.bin/tsc && ./node_modules/.bin/webpack --env=prod && ./node_modules/.bin/webpack --env=dev"
  },
  "author": "Betchi <shinichi.minobe@gmail.com>",
  "contributors": [
    {
      "name": "Betchi",
      "email": "shinichi.minobe@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/swagchat/react-swagchat"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.5",
    "history": "4.6.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scroll": "1.5.2",
    "redux": "3.7.1",
    "swagchat-sdk": "0.9.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.0",
    "@types/history": "4.6.0",
    "@types/jest": "^20.0.8",
    "@types/lodash": "4.14.64",
    "@types/react": "^16.0.5",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "4.4.40",
    "@types/react-router-dom": "4.0.4",
    "@types/react-scroll": "1.5.0",
    "autoprefixer": "7.1.1",
    "code-point-at": "1.1.0",
    "cpx": "^1.5.0",
    "css-loader": "0.28.1",
    "extract-text-webpack-plugin": "2.1.0",
    "jest": "^20.0.4",
    "postcss": "6.0.1",
    "postcss-cssnext": "2.11.0",
    "postcss-easy-import": "2.0.0",
    "postcss-loader": "2.0.5",
    "postcss-modules-values": "^1.3.0",
    "react-hot-loader": "1.3.1",
    "react-test-renderer": "^15.6.1",
    "style-loader": "0.17.0",
    "ts-jest": "^20.0.13",
    "ts-loader": "^2.3.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typed-css-modules": "^0.3.0",
    "typescript": "^2.4.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
